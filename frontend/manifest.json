{
  "manifest_version": 3,
  "name": "Swift Forms",
  "description": "Fill google forms swiftly!",
  "version": "1.0",
  "background": {
    "service_worker": "service-worker.js"
  },
  "action": {
    "default_icon": {
      "32": "images/icon-32.png"
    }
  },
  "permissions": ["tabs", "sidePanel"],
  "side_panel": {
    "default_path": "sidepanel.html"
  }
}
